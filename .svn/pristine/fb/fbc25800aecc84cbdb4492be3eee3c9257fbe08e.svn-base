@charset "UTF-8";

$desktop : 'min-width: 769px';

// Override Bootstrap's defaults
$input-border-radius: 0;
$brand-primary: #2f3b49;
$color-brand-primary: #2f3b49;
$color-brand-secondary: #64dc9a;

$font-family-sans-serif: 'Khula', 'Helvetica Neue', Helvetica, Arial, sans-serif !default;
$input-border-focus: $color-brand-primary !default;
$padding-base-vertical:     8px !default;
$state-danger-text: #d84244 !default;
$input-bg: #f5f5f5;
$input-border: transparent;
$label-color: $color-brand-primary;
$input-color: $color-brand-primary;
$input-border-focus: $color-brand-secondary;


$sidebar-width: 300px;
