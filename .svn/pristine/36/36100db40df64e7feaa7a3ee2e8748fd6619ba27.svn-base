@charset "UTF-8";

// Imports and
@import 'variables';
@import '../node_modules/bootstrap-sass/assets/stylesheets/_bootstrap.scss';
@import 'helpers';
@import 'login';
@import 'menu';

*, *:before, *:after {
  box-sizing: inherit;
}

html{
  box-sizing: border-box;
  -webkit-font-smoothing: antialiased;
  height: 100%;
  color: #2f3b49;
}

::selection {
  color: white;
}

img{
  max-width: 100%;
}
