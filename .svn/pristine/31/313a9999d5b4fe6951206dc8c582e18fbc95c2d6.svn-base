<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Log in</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" href="css/main.css">
  </head>
  <body>
    <div class="gradient-background"></div>

    <div class="login-container">
      <img src="img/icon-login.svg" alt="DentClinic Manager" class="login__logo">
      <form class="form-login">
  			<div class="form-group">
  			  <input class="form-control" type="text" name="user" placeholder="Username">
  			</div>
  			<div class="form-group">
  			  <input class="form-control" type="password" name="pass" placeholder="Password">
          <p class="help-block">
            <a href="#">Forgot your password?</a>
          </p>
  			</div>
  			<input type="submit" name="login" class="btn btn-primary login__submit" value="Log in">
  		</form>
      <div class="login__copy">&copy; Go On Software · 2016 · <a href="#">Contact us</a></div>
    </div>

    <script src="https://code.jquery.com/jquery-1.12.0.min.js"></script>
    <script>window.jQuery || document.write('<script src="js/vendor/jquery-1.12.0.min.js"><\/script>')</script>
    <script src="js/app.js"></script>


  </body>
</html>
